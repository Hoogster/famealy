{
  "name": "famealy",
  "version": "1.0.0",
  "description": "Family meal suggestion web app",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "build": "npm run install-client && cd client && npm run build",
    "start": "node server/index.js",
    "vercel-build": "npm run install-client && cd client && npm run build"
  },
  "keywords": ["meal", "family", "menu", "suggestions"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
